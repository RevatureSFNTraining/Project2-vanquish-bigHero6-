<aura:component controller="bookCardCarouselController">
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <div class="carousel">
        <aura:iteration items="{!v.bookCards}" var="book">
            <div class="carousel__item">
                <!-- <c-alonso-lwc book="book"> -->
                <p>{!book.Name}</p>
            </div>

        </aura:iteration>
      </div>
</aura:component>	
