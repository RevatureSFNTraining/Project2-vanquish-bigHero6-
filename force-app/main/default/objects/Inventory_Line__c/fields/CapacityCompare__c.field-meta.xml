<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CapacityCompare__c</fullName>
    <description>Returns True if current quantity of Inventory Line falls below 20% of Store&apos;s Inventory Capacity</description>
    <externalId>false</externalId>
    <formula>If ( (20/100) &gt;= (Quantity__c / Inventory__r.Capacity__c) , &apos;true&apos;, &apos;false&apos;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>CapacityCompare</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
